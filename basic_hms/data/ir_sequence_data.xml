<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

        <record id="imaging_sequence" model="ir.sequence">
            <field name="name">Solicitud de Imagen</field>
            <field name="code">imaging_seq</field>
            <field name="prefix">IMG</field>
            <field name="padding">3</field>
        </record>

        <record id="inpatient_registration_code_sequence" model="ir.sequence">
            <field name="name">Registro de Pacientes</field>
            <field name="code">medical.inpatient.registration</field>
            <field name="prefix">INPAC</field>
            <field name="padding">3</field>
        </record>

        <record id="medical_appointment_code_sequence" model="ir.sequence">
            <field name="name">Cita Médica</field>
            <field name="code">medical.appointment</field>
            <field name="prefix">APT</field>
            <field name="padding">0</field>
        </record>

        <record id="medical_health_service_code_sequence" model="ir.sequence">
            <field name="name">Servicio Médico</field>
            <field name="code">medical.health_service</field>
            <field name="prefix">SERV%(year)s/</field>
            <field name="padding">3</field>
        </record>

        <record id="patient_registration_sequence" model="ir.sequence">
            <field name="name">Secuencia de Pacientes</field>
            <field name="code">medical.patient</field>
            <field name="prefix">PAC</field>
            <field name="padding">2</field>
        </record>

        <record id="prescription_order_sequence" model="ir.sequence">
            <field name="name">Prescripciones</field>
            <field name="code">medical.prescription.order</field>
            <field name="prefix">PRES%(year)s/</field>
            <field name="padding">6</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="medical_lab_test_sequence" model="ir.sequence">
            <field name="name">Prueba de Laboratorio Médico</field>
            <field name="code">ltest_seq</field>
            <field name="prefix">TEST</field>
            <field name="padding">3</field>
        </record>

        <record id="lab_test_sequence" model="ir.sequence">
            <field name="name">Prueba de Laboratorio</field>
            <field name="code">test_seq</field>
            <field name="prefix">LABT</field>
            <field name="padding">3</field>
        </record>

        <record id="new_born_sequence" model="ir.sequence">
            <field name="name">Recién Nacido</field>
            <field name="code">new_born_seq</field>
            <field name="prefix">NEW</field>
            <field name="padding">3</field>
        </record>

        <record id="medical_health_service_sequence" model="ir.sequence">
            <field name="name">Servicio Médico</field>
            <field name="code">medical_health_ser</field>
            <field name="prefix">MEDSR</field>
            <field name="padding">3</field>
        </record>

        <record id="prescription_invoice_sequence" model="ir.sequence">
            <field name="name">Factura de Prescripción</field>
            <field name="code">pres_inv_seq</field>
            <field name="prefix">PRES/INV/</field>
            <field name="padding">4</field>
        </record>

        <record id="lab_invoice_sequence" model="ir.sequence">
            <field name="name">Factura de Laboratorio</field>
            <field name="code">lab_inv_seq</field>
            <field name="prefix">LAB/INV/</field>
            <field name="padding">4</field>
        </record>

        <record id="medical_appointments_invoice_sequence" model="ir.sequence">
            <field name="name">Factura de Citas Médicas</field>
            <field name="code">medical_app_inv_seq</field>
            <field name="prefix">MED/INV/</field>
            <field name="padding">4</field>
        </record>

        <record id="medical_health_service_invoice_sequence" model="ir.sequence">
            <field name="name">Factura de Servicio Médico</field>
            <field name="code">medical_health_service_inv_seq</field>
            <field name="prefix">MED/HS/INV/</field>
            <field name="padding">4</field>
        </record>

        <record id="medical_lab_test_invoice_sequence" model="ir.sequence">
            <field name="name">Factura de Prueba de Laboratorio Médico</field>
            <field name="code">medical_lab_test_inv_seq</field>
            <field name="prefix">MED/LT/INV/</field>
            <field name="padding">4</field>
        </record>


    </data>
</odoo>
